    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/node": "^20.4.9",
    "@types/react-redux": "^7.1.25",
    "axios": "^1.4.0",
    "cloudpayments": "^6.0.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "draft-js": "^0.11.7",
    "lucide-react": "^0.260.0",
    "moment": "^2.29.4",
    "next": "^13.4.10",
    "next-redux-wrapper": "^8.1.0",
